{
  "name": "@gcba/obelisco-v2",
  "version": "1.0.0",
  "description": "",
  "private": false,
  "main": "dist/styles.css",
  "scripts": {
    "start": "concurrently \"npm run watch-css\" \"cd app && npm run dev\"",
    "watch-css": "nodemon --watch src/styles --ext scss --exec \"npm run build-css\"",
    "build-css": "sass src/styles/main.scss dist/styles.css",
    "build": "npm run build-css",
    "build-versioned": "node build-css-versioned.js",
    "i-app": "cd app && npm install",
    "format": "prettier --write \"src/**/*.scss\"",
    "lint": "stylelint \"src/**/*.scss\"",
    "lint:fix": "stylelint \"src/**/*.scss\" --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^5.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sass": "^1.77.8",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-react": "^7.24.7",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.1.3",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "html-webpack-plugin": "^5.6.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "semantic-release": "^24.0.0",
    "stylelint": "^16.8.2",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  }
}
